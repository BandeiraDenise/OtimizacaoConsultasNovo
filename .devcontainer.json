{
    "name": "Oracle 11g Dev Container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "oracle-11g",
    "workspaceFolder": "/workspace",
    "extensions": [
        "redhat.vscode-yaml",
        "ms-azuretools.vscode-docker",
        "oracle.oracledevtools"
    ],
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "chmod +x /workspace/scripts/*.sh && /workspace/scripts/init.sh",
    "remoteUser": "vscode"
}
